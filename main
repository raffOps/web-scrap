import requests as re
from bs4 import BeautifulSoup as bs

url = "https://busca.movidaseminovos.com.br/filtros/class/usado?sort=11"

r = re.get(url)
soup = bs(r.text, "lxml")

for carro in soup.find_all("div", {"class": "nm-product-info"}):
    print(carro)
    print("#####################################################")
